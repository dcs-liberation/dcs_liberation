    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "axios": "^1.1.2",
    "electron-window-state": "^5.0.3",
