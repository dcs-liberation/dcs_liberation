    "electron": "^21.1.0",
    "electron-is-dev": "^2.0.0",
    "generate-license-file": "^2.0.0",
    "license-checker": "^25.0.1",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.1",
    "wait-on": "^6.0.1"
  }
