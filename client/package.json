    "eject": "react-scripts eject",
    "electron": "wait-on tcp:3000 && electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "leaflet-ruler.d.ts"
  ],
  "prettier": {
    "endOfLine": "auto"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rtk-query/codegen-openapi": "^1.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
